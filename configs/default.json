{
    "server": [
      {
        "client_body_size": 1000,
        "listen": [
          4242,
          4343
        ],
        "server_name": [
          "test.com",
          "test2.com",
          "test3.com"
        ],
        "error_page": [
          "404something"
        ],
        "location /": {
          "allowed_method": [
            "GET",
            "POST",
            "DELETE"
          ],
          "return": "301http://localhost:4242/Redirection/301.html",
          "auto_index": true,
          "index": "index.html",
          "root": "var/www/html"
        },
        "location /test/": {
          "return": "301",
          "root": "var/www/html",
          "allowed_method": [
            "GET"
          ]
        },
        "location /images/": {
            "return": "301",
            "root": "var/www/html",
            "allowed_method": [
              "GET"
            ]
          }
      }
    ]
  }