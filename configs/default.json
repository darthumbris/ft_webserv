{
    "server":
    [
      {
        "client_body_size": 10000000,
        "listen": [4242, 4343],
		    "root": "var/www/html",
        "server_name": [
          "test.com",
          "test2.com",
          "test3.com"
        ],
        "error_page": {
          "404": "404.html",
          "403": "403.html",
          "500": "error.html"
        },
        "location /": {
          "allowed_method": ["GET"],
          "auto_index": true,
          "index": "index.html",
          "root": "var/www/html"
        },
        "location /img/": {
          "allowed_method": ["GET", "DELETE"],
          "auto_index": true,
          "index": "index.html",
          "root": "var/www/html"
        },
        "location /v/": {
          "allowed_method": ["GET"],
          "auto_index": true,
          "index": "index.html",
          "root": "var/www/html"
        },
        "location /cgi_bin/php/": {
          "allowed_method": ["GET", "POST"],
          "root": "var/www/html",
          "CGI": ".php /Users/shoogenb/.brew/bin/php-cgi",
          "upload": "/upload/",
          "return": "301 http://localhost:4242/upload/"
        },
        "location /cgi_bin/python/": {
          "allowed_method": ["GET", "POST"],
          "root": "var/www/html",
          "CGI": ".py /usr/bin/python",
          "upload": "/upload/",
          "return": "301 http://localhost:4242/upload/"
        },
        "location /test/": {
          "root": "var/www/html",
          "auto_index": true,
          "allowed_method": ["GET", "POST"],
          "CGI": ".php /Users/shoogenb/.brew/bin/php-cgi",
          "upload": "/images/"
        },
        "location /test/upload/": {
          "upload": "/images/"
        },
        "location /upload/": {
          "root": "var/www/html",
          "auto_index": true,
          "allowed_method": ["GET", "DELETE"]
        },
        "location /old": {
          "allowed_method": ["GET"],
          "root": "var/www/html",
          "return": "301 http://localhost:4242/"
        }
      }
    ]
}
